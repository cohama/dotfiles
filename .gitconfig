[user]
	name = cohama
	email = hirotaka.hamada.1024@gmail.com
[color]
	ui = auto
[core]
	excludesfile = /home/cohama/.gitignore
	editor = vim
[alias]
	a = add
	aa = add -A
	b = branch
	bd = branch -D
	c = commit -v
	ca = commit -av
	cam = commit --amend
	camc = commit --amend -C HEAD
	co = checkout
	cob = checkout -b
	d = diff
	f = fetch
	l = log --oneline --graph --all --decorate
	m = merge --no-ff --no-commit
	r = reset
	rb = rebase
	rba = rebase --abort
	rbc = rebase --continue
	rbi = rebase --interactive
	rbo = rebase --onto
	rh = reset --hard
	s = status -s

[merge]
	tool = vimdiff
